@model IEnumerable<TRUJILLOISAAC_EXAMENP1.Models.ITrujillo>

@{
    ViewData["Title"] = "Index";
}

<h1>Usuario<</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numfav)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.quierecambiar)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Idceluar)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numfav)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.quierecambiar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Idceluar)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUsuario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdUsuario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdUsuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
